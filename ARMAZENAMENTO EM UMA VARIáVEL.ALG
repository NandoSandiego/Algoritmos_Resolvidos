algoritmo "Armazenamento em uma variável"


var
 nome : vetor [1..10] de caractere
cont : inteiro

inicio

Para cont <- 1 ate 10 faca

     Escreva("Digite um nome: ")
     Leia(nome[cont])
     LimpaTela

FimPara

Para cont <- 1 ate 10 faca

     EscrevaL(cont:2," - ",nome[cont])

FimPara





fimalgoritmo