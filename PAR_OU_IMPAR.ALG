algoritmo "primo ou nao"


var

numero, cont, divi: inteiro


inicio

Escreva("Digite um número: ")
leia(numero)

divi <-0

Para cont <- 1 ate numero faca

     Se (numero %cont = 0 ) entao
        divi <- divi +1
     FimSe


FimPara

 Se (divi = 2) entao
        Escreva("É par" )
 SeNao
      Escreva("É impar")
 FimSe




fimalgoritmo