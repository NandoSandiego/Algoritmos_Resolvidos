algoritmo "imprimindo o maior"
var


num_max, cont, num_maior, numero : inteiro

inicio

Escreva("Contar até: ")
Leia(num_max)

  num_maior <- 0

Para cont<- 1 ate num_max  faca
     Escreva("Digite o ", cont,"º número: ")
     leia(numero)
     
     Se (numero > num_maior) entao
          num_maior <-  numero
     FimSe



FimPara

EscrevaL(num_maior)


fimalgoritmo